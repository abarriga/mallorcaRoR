<div id="wrapper">
<p id="notice"><%= notice %></p>

<p>
  <b>Created by:</b>
  <%= get_user(@task).name %> <%= get_user(@task).surname %>
</p>
<p>
  <b>Title:</b>
  <%= @task.title %>
</p>
<p>
  <b>Description:</b>
  <%= @task.description %>
</p>
<p>
  <b>Finished:</b>
  <%= @task.finished %>
</p>


<%= link_to 'Edit', edit_task_path(@task) %> |
<%= link_to 'Back', tasks_path %>
</div>